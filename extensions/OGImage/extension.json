{
    "name": "OGImage",
    "version": "1.0.0",
    "author": "WikiRomandie",
    "url": "https://wikiromandie.org",
    "description": "Generate OpenGraph images cropped at 1200x630",
    "manifest_version": 2,

    "Hooks": {
        "BeforePageDisplay": [
            "OGImageHooks::onBeforePageDisplay"
        ]
    },

    "AutoloadClasses": {
        "OGImageHooks": "includes/OGImageHooks.php"
    },

    "requires": {
        "MediaWiki": ">=1.39"
    }
}
